@model Venu.Models.Models.Catogory

<form method="post" style="width: 600px; position:absolute">
     <input asp-for="id" hidden/>
    <div class=" border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary p-0">
                Delete Catogory
            </h2>
            <hr />
        </div>
        <div asp-validation-summary="All"></div>
        <div class="mb-3 row ">
            <label asp-for="Name" disabled class="p-0"></label>
            <input asp-for="Name" disabled class="form-control" />

        </div>

        <div class="mb-3 row">
            <label asp-for="DisplayOrder" disabled class="p-0"></label>
            <input asp-for="DisplayOrder" disabled class="form-control" />

        </div>
    </div>
    <div class="row">
        <div class="col-3">
            <button type="submit" class="btn btn-danger form-control">Delete</button>
        </div>
        <div class="col-3">
            <a asp-controller="Catogory" asp-action="Index" class=" btn btnoutline-secondary   form-control">Back to List</a>
        </div>
    </div>



</form>

